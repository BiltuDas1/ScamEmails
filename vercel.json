{
    "redirects": [{ "source": "/", "destination": "/api" }],
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "builds":[
        {
            "src":"python/*.py",
            "use":"vercel@python",
            "config":{
                "runtime":"python3.10"
            }
        }
    ]
}